class A{
  let x = 0;
  let y = 0;

  car(){
    return x + y;
  }
}
class B extends A{
  let a = 0;
  let b = 0;

  bar(){
    return x + y + a + b;
  }
}
main() {
  boo = new B;
  obj = new A;

  obj.x = 11;
  obj.y = 11;

  boo.x = 2;
  boo.y = 3;
  boo.a = 2;
  boo.b = 3;
  
  return obj.car() + boo.bar();
}