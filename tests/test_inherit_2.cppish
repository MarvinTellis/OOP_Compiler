Class A{
  let x = 0;
  let y = 0;

  car(){
    return x + y;
  }
}
Class B extends A{
  let a = 0;
  let b = 0;

  bar(){
    return a + b;
  }
}
main() {
  let boo = new B;
  let obj = new A;
  boo.x = 2;
  boo.y = 3;
  boo.a = 2;
  boo.b = 3;

  obj.x = 10;
  obj.y = 12;
  
  return boo.bar() + obj.car();
}